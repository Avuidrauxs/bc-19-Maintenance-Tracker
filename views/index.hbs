  <script>
    function showForm() {
      var x = document.getElementById('notificat');

      x.style.display = 'block';
      var p = document.getElementById('notificat');
      var sum = p.innerHTML;
      p.innerHTML = ++sum;

    }

    function dispTable(){

      var x = document.getElementById('dispTable');
      var y = document.getElementById('mainDetails');
      var z = document.getElementById('repairDetails');
      if(x.style.display === 'none'){
      x.style.display = 'block';
      y.innerHTML = "Hide Details"
      z.innerHTML = "Hide Details"
    }else{
      x.style.display = 'none';
      y.innerHTML = "View Details"
      z.innerHTML = "Hide Details"
    }

    }
    function decreasePending() {
      var p = document.getElementById('pending_total');
      var sum = p.innerHTML;
      if (sum > 0) {
        p.innerHTML = --sum;
        showForm();
        increaseMaintenance();
      }

    }

    function increaseMaintenance() {
      var p = document.getElementById('maintenance_total');
      var sum = p.innerHTML;
      p.innerHTML = ++sum;
    }
